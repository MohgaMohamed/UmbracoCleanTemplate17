{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Server=MOHGAMOHAMED;Database=MOHPortal.LocalDev2;Connect Timeout=200;Command Timeout=200;Integrated Security=true;TrustServerCertificate=true;",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.SqlClient"
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "75315eed-f90c-4ed2-b0eb-e02f8f854483",
        "SanitizeTinyMce": true,
        "UmbracoPath": "/umbraco"
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Security": {
        "AllowConcurrentLogins": false
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeManual",
        "AcceptUnsafeModelsDirectory": true,
        "ModelsDirectory": "../../Core/MOHPortal.Core.Umbraco/Models/Gen",
        "ModelsNamespace": "MOHPortal.Core.Umbraco.Model.Gen"
      }
    }
  },

  "smidge": {
    "dataFolder": "App_Data/Smidge",
    "environments": {
      "Development": {
        "debug": true
      },
      "Production": {
        "debug": false
      }
    }
  }
}
